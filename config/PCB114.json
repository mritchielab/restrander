{
    "name": "PCB114",
    "description": "Looks for the standard TSO (SSP) and RTP used in PCB114 chemistry.",
    "pipeline": [
        {
            "type": "poly",
            "tail-length": 12,
            "search-size": 200
        },
        {
            "type": "primer",
            "tso": "TTTCTGTTGGTGCTGATATTGCTTT",
            "rtp": "CTTGCCTGTCGCTCTATCTTCAGAGGAG",
            "report-artefacts": true
        }
    ],
    "silent": false,
    "exclude-unknowns": true,
    "error-rate": 0.25
}
